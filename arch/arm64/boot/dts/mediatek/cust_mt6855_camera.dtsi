/* CAMERA EEPROM */
&i2c2 {
	status = "okay";
	clock-frequency = <400000>;
	#address-cells = <1>;
	size-cells = <0>;
	mtk_camera_eeprom2:camera_eeprom2@50 {
		compatible = "mediatek,camera_eeprom";
		reg = <0x50>;
		status = "okay";
	};
	camera_main_three_mtk:camera_main_three@10 {
		compatible = "mediatek,camera_main_three";
		#thermal-sensor-cells = <0>;
		reg = <0x10>;
		status = "okay";
	};
};
